public class CustomerDaoSQL implements CustomerDao {

	private Logger bankLog = Logger.getRootLogger();
	
	Customer extractUser(ResultSet rs) throws SQLException {
		int id = rs.getInt("customer_id");
		String rsUsername = rs.getString("username");
		int rsPinnumber = rs.getInt("pinnumber");
		String role = rs.getString("role");
		return new Customer(id, rsUsername, rsPinnumber, role);
	}
	
	
	
